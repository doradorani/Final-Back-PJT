<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.office.agijagi_back.Mapper.IUserMapper">

    <update id="deleteUser">
        UPDATE
            user
        SET
            status = 0
        WHERE
            email = #{email}
    </update>

</mapper>